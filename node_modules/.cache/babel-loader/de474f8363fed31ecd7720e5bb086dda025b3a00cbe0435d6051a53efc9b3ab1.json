{"ast":null,"code":"import BaseModal from './BaseModal.vue';\nimport ModalHeader from './ModalHeader.vue';\nimport MainDmd from '../../dmd/MainDmd.vue';\nimport SubDmd from '../../dmd/SubDmd.vue';\nimport store from \"../../../stores/store.js\";\nimport { useWpcEditStore } from '../../../stores/wpcEditStore.js';\nconst WpcEditStore = useWpcEditStore(store);\nexport default {\n  name: 'SpriteModal',\n  components: {\n    BaseModal,\n    ModalHeader,\n    MainDmd,\n    SubDmd\n  },\n  setup() {},\n  data() {\n    return {\n      doBlend: true,\n      doMerge0: false,\n      doMerge1: false,\n      blendOffset: 1,\n      merge0Offset: 2,\n      merge1Offset: 3\n    };\n  },\n  props: {\n    show: Boolean,\n    index: Number\n  },\n  methods: {\n    plane0() {\n      return this.mainPlane();\n    },\n    plane1() {\n      if (this.doBlend) {\n        return this.blendPlane();\n      }\n      return this.mainPlane();\n    },\n    mainPlane() {\n      if (this.doMerge0) {\n        console.log(\"merge0\");\n        const plane0 = WpcEditStore.getFullFrameImageAt(this.index);\n        const plane1 = WpcEditStore.getFullFrameImageAt(this.index + this.merge0Offset);\n        const imageSize = Math.min(plane0.image.length, plane1.image.length, plane1.mask.length);\n        let mergedImage = new Uint8Array(imageSize);\n        let i;\n        for (i = 0; i < imageSize; i++) {\n          if (plane1.mask[i] == 0xFF) {\n            mergedImage.set([plane0.image[i]], i);\n          } else {\n            mergedImage.set([plane1.image[i]], i);\n          }\n        }\n        return mergedImage;\n\n        //return WpcEditStore.mergePlanes(plane0,plane1);\n      }\n\n      return WpcEditStore.getFullFrameImageAt(this.index).image;\n    },\n    blendPlane() {\n      if (this.doMerge1) {\n        console.log(\"merge1\");\n        const plane0 = WpcEditStore.getFullFrameImageAt(this.index + this.blendOffset);\n        const plane1 = WpcEditStore.getFullFrameImageAt(this.index + this.merge1Offset);\n        return WpcEditStore.mergePlanes(plane0, plane1);\n      }\n      return WpcEditStore.getFullFrameImageAt(this.index + this.blendOffset).image;\n    },\n    merge0Plane() {\n      return WpcEditStore.getFullFrameImageAt(this.index + this.merge0Offset).image;\n    },\n    merge1Plane() {\n      return WpcEditStore.getFullFrameImageAt(this.index + this.merge1Offset).image;\n    },\n    setBlendStatus(state) {\n      if (state == true) {\n        this.doBlend = true;\n      } else if (state == false) {\n        this.doBlend = false;\n      }\n    },\n    setMerge0Status(state) {\n      if (state == true) {\n        this.doMerge0 = true;\n      } else if (state == false) {\n        this.doMerge0 = false;\n      }\n    },\n    setMerge1Status(state) {\n      if (state == true) {\n        this.doMerge1 = true;\n      } else if (state == false) {\n        this.doMerge1 = false;\n      }\n    },\n    goToNextBlendPlane() {\n      this.blendOffset++;\n    },\n    goToPrevBlendPlane() {\n      this.blendOffset--;\n    },\n    goToNextMerge0Plane() {\n      this.merge0Offset++;\n    },\n    goToPrevMerge0Plane() {\n      this.merge0Offset--;\n    },\n    goToNextMerge1Plane() {\n      this.merge1Offset++;\n    },\n    goToPrevMerge1Plane() {\n      this.merge1Offset--;\n    }\n  },\n  updated() {\n    // reset the values (or get from a store when that ha been implemented)\n    this.doBlend = true;\n    this.doMerge0 = false;\n    this.doMerge1 = false;\n    this.blendOffset = 1;\n    this.merge0Offset = 2;\n    this.merge1Offset = 3;\n  }\n};","map":{"version":3,"mappings":"AAiDA,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,MAAK,MAAO,sBAAsB;AACzC,OAAOC,KAAI,MAAO,0BAA0B;AAC5C,SAASC,eAAc,QAAS,iCAAgC;AAEhE,MAAMC,YAAW,GAAID,eAAe,CAACD,KAAK,CAAC;AAE3C,eAAe;EACXG,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRR,SAAS;IACTC,WAAW;IACXC,OAAO;IACPC;EACJ,CAAC;EACDM,KAAK,GAAG,CACR,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,IAAI;M