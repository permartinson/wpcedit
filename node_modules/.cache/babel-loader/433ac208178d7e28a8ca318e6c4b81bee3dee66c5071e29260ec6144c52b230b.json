{"ast":null,"code":"export default {\n  name: 'SubDmd',\n  props: {\n    plane: Uint8Array\n  },\n  methods: {\n    drawImage() {\n      //const DISPLAY_W = 512;\n      //const DISPLAY_H = 128;\n      const DOT_SIZE = 4;\n      const canvas = this.$refs.dmd;\n      const ctx = canvas.getContext(\"2d\");\n      for (let i = 0; i < 128 * 32; i++) {\n        const dotX = i % 128 * DOT_SIZE + DOT_SIZE / 2;\n        const dotY = Math.floor(i / 128) * DOT_SIZE + DOT_SIZE / 2;\n        let color = this.getDotValue(this.plane, i);\n        const red = color * 187 + 33;\n        const green = color * 100 + 33;\n        const blue = 33; //color;\n\n        ctx.beginPath();\n        ctx.arc(dotX, dotY, DOT_SIZE / 2 * 0.9, 0, 2 * Math.PI, false);\n        //ctx.fillStyle = '#F29540';\n        ctx.fillStyle = `rgba(${red}, ${green}, ${blue}, 255)`;\n        ctx.ctx.fill();\n\n        /*\n                    counter++;*/\n      }\n    },\n\n    getDotValue(plane0Data, index) {\n      const width = 128;\n      const col = index % width;\n      const byte = Math.floor(index / 8);\n      const bitPos = col % 8;\n      const bit = (plane0Data[byte] & 0xFF) >> bitPos & 1;\n      return bit;\n    }\n  },\n  mounted() {\n    this.drawImage();\n  },\n  updated() {\n    this.drawImage();\n  }\n};","map":{"version":3,"mappings":"AAQA,eAAe;EACbA,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE;IACLC,KAAK,EAAEC;EACT,CAAC;EACDC,OAAO,EAAE;IACPC,SAAS,GAAG;MACR;MACA;MACA,MAAMC,QAAO,GAAI,CAAC;MAElB,MAAMC,MAAK,GAAI,IAAI,CAACC,KAAK,CAACC,GAAG;MAC7B,MAAMC,GAAE,GAAIH,MAAM,CAACI,UAAU,CAAC,IAAI,CAAC;MACnC,KAAI,IAAIC,CAAC,GAAC,CAAC,EAAEA,CAAC,GAAC,GAAG,GAAC,EAAE,EAAEA,CAAC,EAAE,EAAE;QACxB,MAAMC,IAAG,GAAKD,IAAI,GAAG,GAAIN,QAAO,GAAIA,QAAQ,GAAC,CAAC;QAC9C,MAAMQ,IAAG,GAAIC,IAAI,CAACC,KAAK,CAACJ,IAAI,GAAG,IAAIN,QAAO,GAAIA,QAAQ,GAAC,CAAC;QACxD,IAAIW,KAAI,GAAI,IAAI,CAACC,WAAW,CAAC,IAAI,CAAChB,KAAK,EAACU,CAAC,CAAC;QAC1C,MAAMO,GAAE,GAAIF,KAAK,GAAC,GAAE,GAAG,EAAE;QACzB,MAAMG,KAAI,GAAIH,KAAK,GAAC,GAAE,GAAG,EAAE;QAC3B,MAAMI,IAAG,GAA