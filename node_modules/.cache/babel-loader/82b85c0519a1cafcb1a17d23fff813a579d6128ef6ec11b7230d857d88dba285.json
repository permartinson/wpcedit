{"ast":null,"code":"import BaseModal from './BaseModal.vue';\nimport ModalHeader from './ModalHeader.vue';\nimport MainDmd from '../../dmd/MainDmd.vue';\nimport SubDmd from '../../dmd/SubDmd.vue';\nimport OffsetControl from '../../ui/OffsetControl.vue';\nimport store from \"../../../stores/store.js\";\nimport { useWpcEditStore } from '../../../stores/wpcEditStore.js';\nconst WpcEditStore = useWpcEditStore(store);\nexport default {\n  name: 'SpriteModal',\n  components: {\n    BaseModal,\n    ModalHeader,\n    MainDmd,\n    SubDmd,\n    OffsetControl\n  },\n  setup() {},\n  data() {\n    return {\n      doBlend: false,\n      offset: 0,\n      blendOffset: 1,\n      xOffset0: 0,\n      yOffset0: 0,\n      xOffset1: 0,\n      yOffset1: 0\n    };\n  },\n  props: {\n    show: Boolean,\n    index: Number,\n    table: Number\n  },\n  computed: {\n    planeIndices() {\n      const currIndex = Math.max(this.index + this.offset, 0); // BUG! need to use the map as a reference here\n      return {\n        current: currIndex,\n        blend: Math.max(currIndex + this.blendOffset, 0)\n      };\n    },\n    currPlaneTitle() {\n      return `This sprite, index ${this.planeIndices.current}`;\n    },\n    blendPlaneLabel() {\n      return `Blend with index ${this.planeIndices.blend}`;\n    }\n  },\n  methods: {\n    plane0() {\n      const indices = this.planeIndices;\n      const sprite = WpcEditStore.getSpriteAt(this.table, indices.current);\n      const xOffset = Math.floor((128 - sprite.width) / 2);\n      const yOffset = Math.floor((32 - sprite.height) / 2);\n      return WpcEditStore.getSpriteAsFullFrame(sprite, xOffset, yOffset);\n    },\n    plane1() {\n      if (this.doBlend) {\n        return this.blendPlane();\n      }\n      return this.plane0();\n    },\n    blendPlane() {\n      const indices = this.planeIndices;\n      const sprite = WpcEditStore.getSpriteAt(this.table, indices.blend);\n      const xOffset = Math.floor((128 - sprite.width) / 2);\n      const yOffset = Math.floor((32 - sprite.height) / 2);\n      return WpcEditStore.getSpriteAsFullFrame(sprite, xOffset, yOffset);\n    },\n    setBlendStatus(state) {\n      if (state == true) {\n        this.doBlend = true;\n      } else if (state == false) {\n        this.doBlend = false;\n      }\n    },\n    goToNextBlendPlane() {\n      this.blendOffset++;\n    },\n    goToPrevBlendPlane() {\n      this.blendOffset--;\n    },\n    goNext() {\n      this.offset++;\n      this.setInitValues();\n    },\n    goPrev() {\n      if (this.planeIndices.current > 0) {\n        this.offset--;\n      }\n      this.setInitValues();\n    },\n    goN2() {\n      this.offset += 2;\n      this.setInitValues();\n    },\n    goP2() {\n      if (this.planeIndices.current > 0) {\n        this.offset -= 2;\n      }\n      this.setInitValues();\n    },\n    setInitValues() {\n      // reset the values (or get from a store when that ha been implemented)\n      this.blendOffset = 1;\n      this.doBlend = false;\n    }\n  },\n  updated() {\n    this.offset = 0;\n    this.setInitValues();\n  }\n};","map":{"version":3,"mappings":"AAqDA,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,MAAK,MAAO,sBAAsB;AACzC,OAAOC,aAAY,MAAO,4BAA4B;AACtD,OAAOC,KAAI,MAAO,0BAA0B;AAC5C,SAASC,eAAc,QAAS,iCAAgC;AAEhE,MAAMC,YAAW,GAAID,eAAe,CAACD,KAAK,CAAC;AAE3C,eAAe;EACXG,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRT,SAAS;IACTC,WAAW;IACXC,OAAO;IACPC,MAAM;IACNC;EACJ,CAAC;EACDM,KAAK,GAAG,CACR,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE,CAAC;MACTC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE,CAAC;MACXC,QAAQ,EAAE,CAAC;MACXC,QAAQ,EAAE,CAAC;MACXC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,KAAK,EAAE;IACHC,IAAI,EAAEC,OAAO;IACbC,KAAK,EAAEC,MAAM;IACbC,KAAK,EAAED;EACX,CAAC;EACDE,QAAQ,EAAE;IACNC,YAAY,GAAG;MACX,MAAMC,SAAQ,GAAIC,IAAI,CAACC,GAAG,CAAC,IAAI,CAACP,KAAK,GAAC,IAAI,CAACT,MAAM,EAAC,CAAC,CAAC,EAAE;MACtD,OAAO;QACHiB,OAAO,EAAEH,SAAS;QAClBI,KAAK,EAAEH,IAAI,CAACC,GAAG,CAACF,SAAS,GAAC,IAAI,CAACb,WAAW,EAAC,C