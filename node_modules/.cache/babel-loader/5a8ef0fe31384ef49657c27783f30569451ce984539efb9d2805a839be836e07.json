= true;\n            }\n          }\n        },\n        agreeClicked() {\n          if(this.tempFile != null) {\n            this.readBytes(this.tempFile);\n          }\n          this.warnings.load++;\n          this.showDisclaimer = false;\n        },\n        disagreeClicked() {\n          this.tempFile = null;\n          WpcEditStore.unloadRom();\n          this.showDisclaimer = false;\n        },\n        okClicked() {\n          this.showError = false;\n        },\n        readBytes(file) {\n          let status = true;\n          const reader = new FileReader();\n          reader.addEventListener(\"loadend\", () => {\n              if(reader.result) {\n                const bytes = new Uint8Array(reader.result);\n                const result = WpcEditStore.loadRom(bytes, file.name);\n                if(result.error) {\n                  this.errorMessage = result.msg;\n                  this.showError = true;\n                  status = false;\n                }\n              }\n          });\n          reader.readAsArrayBuffer(file);\n          return status;\n        },\n      }\n    }\n  \n  </script>\n  \n  <style scoped>\n  .navigation {\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    margin-top: var(--space-xl);\n  }\n  .navigation a {\n    text-decoration: none;\n  }\n  .alert-ok-btn {\n    width: 12rem;\n    margin: 0 auto;\n  }\n  </style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}