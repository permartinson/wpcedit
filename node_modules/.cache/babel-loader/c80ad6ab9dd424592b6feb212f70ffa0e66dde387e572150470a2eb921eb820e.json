t CheckBox from '../ui/CheckBox.vue';\nimport DirectionalButton from '../ui/buttons/DirectionalButton.vue';\nimport SmallIcon from '../icons/SmallIcon.vue';\n\nexport default {\n  name: 'SubDmd',\n  components: {\n    CheckBox\n  },\n  props: {\n    plane: Uint8Array,\n    title: String,\n    checkbox: Boolean,\n    navBtns: Boolean,\n    DirectionalButton,\n    SmallIcon\n  },\n  methods: {\n    drawImage() {\n        const DOT_SIZE = 4;\n        const canvas = this.$refs.dmd;\n        const ctx = canvas.getContext(\"2d\");\n        for(let i=0; i<128*32; i++) {\n            const dotX = (i % 128) * DOT_SIZE + DOT_SIZE/2;\n            const dotY = Math.floor(i / 128) * DOT_SIZE + DOT_SIZE/2;\n            const dotVal = this.getDotValue(this.plane,i);\n            ctx.beginPath();\n            ctx.arc(dotX, dotY, DOT_SIZE/2*0.9, 0, 2 * Math.PI, false);\n            ctx.fillStyle = `rgba(220, 133, 33, ${dotVal*255})`;\n            ctx.fill();\n        }\n    },\n    getDotValue(plane0Data,index) {\n        const width = 128;\n        const col = index % width;\n        const byte = Math.floor(index/8);\n        const bitPos = (col % 8);\n        const bit =  ( plane0Data[byte] & 0xFF ) >>(bitPos) & 1;\n        return bit;\n    }\n  },\n  mounted() {\n    this.drawImage();\n  },\n  updated() {\n    this.drawImage();\n  }\n}\n\n</script>\n\n<style scoped>\n.controls {\n  height: 3.2rem;\n}\n.dmd-container {\n  background: var(--black);\n}\n.dmd {\n  width: 100%;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}