{"ast":null,"code":"import BaseModal from './BaseModal.vue';\nimport MainDmd from '../../dmd/MainDmd.vue';\nimport SubDmd from '../../dmd/SubDmd.vue';\nimport CheckBox from '../../ui/CheckBox.vue';\nimport store from \"../../../stores/store.js\";\nimport { useWpcEditStore } from '../../../stores/wpcEditStore.js';\nconst WpcEditStore = useWpcEditStore(store);\nexport default {\n  name: 'SpriteModal',\n  components: {\n    BaseModal,\n    MainDmd,\n    SubDmd,\n    CheckBox\n  },\n  setup() {},\n  data() {\n    return {\n      doBlend: false\n    };\n  },\n  props: {\n    show: Boolean,\n    index: Number,\n    table: Number\n  },\n  methods: {\n    plane0() {\n      return WpcEditStore.getSpriteAt(this.table, this.index);\n    },\n    plane1() {\n      //console.log(\"plane1: \"+this.this.index+1)\n      if (this.doBlend) {\n        return this.blendPlane();\n      }\n      return WpcEditStore.getSpriteAt(this.table, this.index);\n    },\n    blendPlane() {\n      return WpcEditStore.getSpriteAt(this.table, this.index + 1);\n    },\n    setBlendStatus(state) {\n      console.log(state.returnValue);\n      if (state == true) {\n        // temp, to make sure the values are correct. cleanup later\n        this.doBlend = true;\n      } else if (state == false) {\n        this.doBlend = false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAYA,OAAOA,SAAQ,MAAO,iBAAiB;AACvC,OAAOC,OAAM,MAAO,uBAAuB;AAC3C,OAAOC,MAAK,MAAO,sBAAsB;AACzC,OAAOC,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,KAAI,MAAO,0BAA0B;AAC5C,SAASC,eAAc,QAAS,iCAAgC;AAEhE,MAAMC,YAAW,GAAID,eAAe,CAACD,KAAK,CAAC;AAE3C,eAAe;EACXG,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRR,SAAS;IACTC,OAAO;IACPC,MAAM;IACNC;EACJ,CAAC;EACDM,KAAK,GAAG,CACR,CAAC;EACDC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE;IACb;EACJ,CAAC;EACDC,KAAK,EAAE;IACHC,IAAI,EAAEC,OAAO;IACbC,KAAK,EAAEC,MAAM;IACbC,KAAK,EAAED;EACX,CAAC;EACDE,OAAO,EAAE;IACLC,MAAM,GAAG;MACL,OAAOb,YAAY,CAACc,WAAW,CAAC,IAAI,CAACH,KAAK,EAAC,IAAI,CAACF,KAAK,CAAC;IAC1D,CAAC;IACDM,MAAM,GAAG;MACL;MACA,IAAG,I